<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <ImageView
            android:id="@+id/imageView5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_marginRight="24dp"
            android:layout_marginTop="24dp"
            android:layout_toEndOf="@+id/performance"
            android:adjustViewBounds="true"
            android:scaleType="fitCenter"
            app:srcCompat="@drawable/ic_zirbl" />

        <hsaugsburg.zirbl001.Fonts.QuicksandBoldPrimaryView
            android:id="@+id/quicksandRegularPrimaryView"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="10dp"
            android:layout_marginLeft="24dp"
            android:layout_marginTop="16dp"
            android:text="Zoo"
            app:textAllCaps="false" />

        <hsaugsburg.zirbl001.Fonts.QuicksandRegularPrimaryView
            android:id="@+id/quicksandRegularPrimaryView2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/quicksandRegularPrimaryView"
            android:layout_marginBottom="10dp"
            android:layout_marginLeft="24dp"
            android:text="25. September 2017"
            app:textAllCaps="false" />

        <LinearLayout
            android:id="@+id/ranking"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/quicksandRegularPrimaryView2"
            android:layout_marginBottom="10dp"
            android:layout_marginLeft="24dp"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/imageView2"
                android:layout_width="20dp"
                android:layout_height="20dp"
                app:srcCompat="@drawable/ic_time" />

            <hsaugsburg.zirbl001.Fonts.QuicksandBoldPrimaryView
                android:id="@+id/rankingText"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="16dp"
                android:text="1h 40min"
                app:textAllCaps="false" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/performance"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/ranking"
            android:layout_marginBottom="10dp"
            android:layout_marginLeft="24dp"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/imageView3"
                android:layout_width="20dp"
                android:layout_height="20dp"
                app:srcCompat="@drawable/ic_cup" />

            <hsaugsburg.zirbl001.Fonts.QuicksandBoldPrimaryView
                android:id="@+id/performanceTeamname"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="16dp"
                android:text="4. Platz von 255"
                app:textAllCaps="false" />
        </LinearLayout>

        <hsaugsburg.zirbl001.Fonts.QuicksandBoldPrimaryView
            android:id="@+id/quicksandRegularPrimaryView3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/performance"
            android:layout_marginLeft="24dp"
            android:text="Die Namenlosen:"
            app:textAllCaps="false" />

        <hsaugsburg.zirbl001.Fonts.QuicksandRegularPrimaryView
            android:id="@+id/quicksandRegularPrimaryView4"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/quicksandRegularPrimaryView3"
            android:layout_marginBottom="16dp"
            android:layout_marginLeft="24dp"
            android:text="Meli, Tobi, Coco, Vreni, Lari, Simon, Tim, Dini, Moe, Melis"
            app:textAllCaps="false" />

    </RelativeLayout>

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_below="@+id/quicksandRegularPrimaryView3"
        app:srcCompat="@color/colorLine" />

</LinearLayout>